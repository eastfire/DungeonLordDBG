.hide-detail .detail {
	display:none;
}

.detail {
	color:black;
	text-shadow: 1px 1px 0px white;
	font-weight: bold;
	text-align: center;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: -moz-none;
	-ms-user-select: none;
	user-select: none;
	list-style-type:"";
}

label {
	color:black;
	text-shadow: 1px 1px 0px white;
	font-weight: bold;
	text-align: center;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: -moz-none;
	-ms-user-select: none;
	user-select: none;
	list-style-type:"";
}

